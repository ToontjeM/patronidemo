[Unit]
Description=etcd
Documentation=https://github.com/coreos/etcd
Conflicts=etcd.service
Conflicts=etcd2.service

[Service]
Type=notify
Restart=always
RestartSec=5s
LimitNOFILE=40000
TimeoutStartSec=0

ExecStart=/usr/local/bin/etcd --name e3 \
  --data-dir /var/lib/etcd \
  --listen-client-urls http://192.168.56.23:2379 \
  --advertise-client-urls http://192.168.56.23:2379 \
  --listen-peer-urls http://192.168.56.23:2380 \
  --initial-advertise-peer-urls http://192.168.56.23:2380 \
  --initial-cluster e1=http://192.168.56.21:2380,e2=http://192.168.56.22:2380,e3=http://192.168.56.23:2380 \
  --initial-cluster-token cluster1 \
  --initial-cluster-state new

[Install]
WantedBy=multi-user.target